{
		"version": "0.2.0",
		"configurations": [
			{
				"name": "Python",
				"type": "python",
				"request": "launch",
				"stopOnEntry": true,
				"pythonPath": "${config:python.pythonPath}",
				"program": "${file}",
				"cwd": "",
				"console": "externalTerminal",
				"env": {},
				"envFile": "${workspaceRoot}/.env",
				"debugOptions": [
					"WaitOnAbnormalExit",
					"WaitOnNormalExit"
				]
			},
			{
				"name": "(gdb) Launch",
				"type": "cppdbg",
				"request": "launch",
				"program": "${workspaceRoot}/a.out",
				"args": [],
				"stopAtEntry": false,
				"cwd": "${workspaceRoot}",
				"environment": [],
				"externalConsole": true,
				"MIMode": "gdb",
				"setupCommands": [
					{
						"description": "Enable pretty-printing for gdb",
						"text": "-enable-pretty-printing",
						"ignoreFailures": true
					}
				]
			}
		]
	}
